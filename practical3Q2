#include<stdio.h>
int main()
{
    int num1,num2,num3;
    printf("enter three integers");
    scanf("%d %d %d",&num1,&num2,&num3);

    int max=num1;
    int min=num2;

    if(num2>max){
        max=num2;
    }
    if(num3>max){
        max=num3;
    }
    printf("The largest value is %d\n",max);

    if(num2<min){
        min=num2;
    }
    if(num3<min){
        min=num3;
    }
    printf("The smallest value is%d\n",min);
}

